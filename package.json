{
  "name": "dashboard",
  "private": true,
  "version": "2.0.7",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3002 --host",
    "build": "tsc && vite build",
    "build:dev": "tsc && vite build --mode development",
    "build:prod": "tsc && vite build --mode production",
    "serve": "vite preview --port 3000 --host",
    "preview": "vite preview",
    "lint": "npx @biomejs/biome check",
    "lint:staged": "npx @biomejs/biome check --staged --no-errors-on-unmatched",
    "lint:fix": "npx @biomejs/biome check --write --unsafe",
    "postinstall": "patch-package && yarn gen-proto",
    "execute-husky": "npx husky install && npx husky add .husky/pre-commit \"npm run lint:staged\"",
    "prepare": "husky install",
    "i18n": "i18next",
    "gen-proto": "./scripts/gen-proto.sh",
    "locize-sync": "i18next && node scripts/locize-sync.js",
    "locize-cleanup": "i18next && node scripts/locize-cleanup.js",
    "generate-sitemap": "node scripts/generate-sitemap.js"
  },
  "engines": {
    "node": "20"
  },
  "imports": {
    "#index.css": "./src/styles/index.css"
  },
  "dependencies": {
