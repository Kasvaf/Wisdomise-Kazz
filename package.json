{
  "name": "dashboard",
  "private": true,
  "version": "2.0.6",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3001 --host",
    "build": "tsc && vite build",
    "build:dev": "tsc && vite build --mode development",
    "build:prod": "tsc && vite build --mode production",
    "serve": "vite preview --port 3000 --host",
    "preview": "vite preview",
    "lint": "eslint --quiet ./src --ext .jsx,.js,.tsx,.ts",
    "postinstall": "patch-package",
    "execute-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\"",
    "prepare": "husky install",
    "i18n": "i18next",
    "sync-locize": "i18next && node sync-locize.js"
  },
  "dependencies": {
    "@ant-design/plots": "^1.2.5",
    "@mdxeditor/editor": "^3.4.0",
    "@microsoft/fetch-event-source": "^2.0.1",
    "@nivo/pie": "^0.83.0",
    "@segment/analytics-next": "^1.59.0",
    "@sentry/react": "^7.45.0",
    "@sentry/tracing": "^7.45.0",
    "@stripe/react-stripe-js": "^2.3.1",
    "@stripe/stripe-js": "^2.1.6",
    "@sumsub/websdk-react": "^2.0.1",
    "@tailwindcss/typography": "^0.5.9",
    "@tanstack/react-query": "^4.29.14",
    "@tanstack/react-query-devtools": "^5.7.2",
    "@web3modal/wagmi": "^3.2.0",
    "antd": "^5.6.2",
    "axios": "^1.4.0",
    "boxicons-quasar": "^1.0.3",
    "clsx": "^1.2.1",
    "dayjs": "^1.11.9",
    "echarts": "^5.4.3",
    "echarts-for-react": "^3.0.2",
    "i18next": "^23.6.0",
    "i18next-browser-languagedetector": "^7.1.0",
    "lightweight-charts": "^4.1.4",
    "numerable": "^0.3.15",
    "overlayscrollbars": "^2.5.0",
    "overlayscrollbars-react": "^0.5.4",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "react": "^18.2.0",
    "react-animate-height": "^3.2.3",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-ga4": "^2.1.0",
    "react-hook-form": "^7.51.4",
    "react-i18next": "^13.3.1",
    "react-qr-code": "^2.0.11",
    "react-router-dom": "^6.9.0",
