{
  "name": "dashboard",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3001 --host",
    "build": "tsc && vite build",
    "build:dev": "tsc && vite build --mode development",
    "build:prod": "tsc && vite build --mode production",
    "serve": "vite preview --port 3000 --host",
    "preview": "vite preview",
    "lint": "eslint --quiet ./src --ext .jsx,.js,.tsx,.ts",
    "execute-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/plots": "^1.2.5",
    "@linaria/core": "^4.2.10",
    "@linaria/react": "^4.3.8",
    "@nivo/pie": "^0.83.0",
    "@reduxjs/toolkit": "^1.9.3",
    "@sentry/react": "^7.45.0",
    "@sentry/tracing": "^7.45.0",
    "@sumsub/websdk-react": "^1.3.7",
    "@tanstack/react-query": "^4.29.14",
    "@types/numeral": "^2.0.2",
    "antd": "^5.6.2",
    "axios": "^1.4.0",
    "clsx": "^1.2.1",
    "crisp-sdk-web": "^1.0.19",
    "dayjs": "^1.11.9",
    "numerable": "^0.3.15",
    "numeral": "^2.0.6",
    "postcss": "^8.4.21",
    "postcss-preset-env": "^8.2.0",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-date-range": "^1.4.0",
    "react-dom": "^18.2.0",
    "react-ga4": "^2.1.0",
    "react-hotjar": "^5.5.0",
    "react-qr-code": "^2.0.11",
    "react-redux": "^8.0.5",
    "react-responsive": "^9.0.2",
    "react-router-dom": "^6.9.0",
    "sass": "^1.62.1",
    "tailwind-merge": "^1.10.0",
    "tailwindcss-animation-delay": "^1.0.7",
    "tw-elements": "^1.0.0-beta1",
    "vite-plugin-svgr": "^2.4.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@linaria/babel-preset": "^4.4.5",
    "@linaria/vite": "^4.2.11",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.192",
    "@types/react": "^18.0.28",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-date-range": "^1.4.4",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tailwindcss": "^3.10.3",
    "husky": "^8.0.3",
    "knip": "^2.14.3",
    "lint-staged": "^13.2.0",
    "prettier": "2.8.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.9.3",
    "vite": "^4.2.0"
  },
  "lint-staged": {
    "src/**/*.{scss,js,jsx,ts,tsx}": [
      "eslint",
      "prettier"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}