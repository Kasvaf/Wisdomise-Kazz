    "husky": "^8.0.3",
    "i18next-parser": "^8.9.0",
    "knip": "^2.19.4",
    "lint-staged": "^13.2.0",
    "postcss": "^8.4.21",
    "postcss-preset-env": "^8.2.0",
    "prettier": "3.0.2",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "tailwindcss": "^3.4.1",
    "typescript": "*",
    "vite": "^4.2.0",
    "vite-plugin-svgr": "^2.4.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --check"
    ],
    "**/*.{html,css}": [
      "prettier --check"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
