{
  "name": "dashboard",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3001 --host",
    "build": "tsc && vite build",
    "build:dev": "tsc && vite build --mode development",
    "build:prod": "tsc && vite build --mode production",
    "serve": "vite preview --port 3000 --host",
    "preview": "vite preview",
    "lint": "eslint --quiet ./src --ext .jsx,.js,.tsx,.ts",
    "execute-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/plots": "^1.2.5",
    "@nivo/pie": "^0.83.0",
    "@sentry/react": "^7.45.0",
    "@sentry/tracing": "^7.45.0",
    "@tailwindcss/typography": "^0.5.9",
    "@tanstack/react-query": "^4.29.14",
    "antd": "^5.6.2",
    "axios": "^1.4.0",
    "clsx": "^1.2.1",
    "crisp-sdk-web": "^1.0.19",
    "dayjs": "^1.11.9",
    "numerable": "^0.3.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-ga4": "^2.1.0",
    "react-hotjar": "^5.5.0",
    "react-qr-code": "^2.0.11",
    "react-router-dom": "^6.9.0",
    "react-transition-group": "^4.4.5",
    "tailwind-merge": "^1.10.0",
    "tailwindcss-animation-delay": "^1.0.7",
    "tw-elements": "^1.0.0-beta1",
    "usehooks-ts": "^2.9.1",
    "vite-plugin-markdown": "^2.1.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@linaria/babel-preset": "^4.4.5",
    "@linaria/core": "^4.2.10",
    "@linaria/react": "^4.3.8",
    "@linaria/vite": "^4.2.11",
    "@types/react": "^18.0.28",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-dom": "^18.0.11",
    "@types/react-transition-group": "^4.4.6",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "husky": "^8.0.3",
    "knip": "^2.19.4",
    "lint-staged": "^13.2.0",
    "postcss": "^8.4.21",
    "postcss-preset-env": "^8.2.0",
    "prettier": "3.0.2",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "sass": "^1.62.1",
    "tailwindcss": "^3.3.2",
    "typescript": "*",
    "vite": "^4.2.0",
    "vite-plugin-svgr": "^2.4.0"
  },
  "lint-staged": {
    "src/**/*.{scss,js,jsx,ts,tsx}": [
      "eslint",
      "prettier"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
