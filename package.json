{
  "name": "dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3001",
    "build": "tsc && vite build",
    "build:dev": "tsc && vite build --mode development",
    "build:prod": "tsc && vite build --mode production",
    "serve": "vite preview --port 3000 --host",
    "preview": "vite preview",
    "lint": "eslint ./src --ext .jsx,.js,.tsx,.ts",
    "execute-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/plots": "^1.2.5",
    "@lottiefiles/react-lottie-player": "^3.5.3",
    "@reduxjs/toolkit": "^1.9.3",
    "@sentry/react": "^7.45.0",
    "@sentry/tracing": "^7.45.0",
    "@sumsub/websdk-react": "^1.3.7",
    "@types/numeral": "^2.0.2",
    "antd": "^4.24.8",
    "autoprefixer": "^10.4.14",
    "aws-amplify": "^5.0.22",
    "dayjs": "^1.11.7",
    "launchdarkly-react-client-sdk": "^3.0.4",
    "numeral": "^2.0.6",
    "postcss": "^8.4.21",
    "postcss-preset-env": "^8.2.0",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-date-range": "^1.4.0",
    "react-dom": "^18.2.0",
    "react-ga4": "^2.1.0",
    "react-hotjar": "^5.5.0",
    "react-notifications": "^1.7.4",
    "react-qr-code": "^2.0.11",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "tailwind-merge": "^1.10.0",
    "tailwindcss-animation-delay": "^1.0.7",
    "tw-elements": "^1.0.0-beta1",
    "usehooks-ts": "^2.9.1",
    "vite-plugin-svgr": "^2.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.192",
    "@types/react": "^18.0.28",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-date-range": "^1.4.4",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tailwindcss": "^3.10.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "prettier": "2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.5",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.3",
    "vite": "^4.2.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint . --fix",
      "prettier --write ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}